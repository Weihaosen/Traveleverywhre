<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    android:background="@color/white"
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <compp.cumulus.traveleverywhre.widget.CircleImageView
        android:id="@+id/img_touxiang"
        android:scaleType="centerCrop"
        android:src="@drawable/tt"
        android:layout_marginTop="@dimen/dp_20"
        android:layout_marginLeft="@dimen/dp_20"
        android:layout_width="@dimen/dp_40"
        android:layout_height="@dimen/dp_40" />
    <TextView
        android:id="@+id/text_mingzi"
        android:layout_marginLeft="@dimen/dp_70"
        android:layout_marginTop="@dimen/dp_30"
        android:text="@string/banbenceshi"
        android:textSize="@dimen/dp_15"
        android:textColor="@color/c_999999"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/text_shijian"
        android:layout_alignParentEnd="true"
        android:text="@string/banbenceshi"
        android:layout_marginEnd="@dimen/dp_10"
        android:layout_marginTop="@dimen/dp_30"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/text_neirong"
        android:layout_marginTop="@dimen/dp_80"
        android:textSize="@dimen/dp_16"
        android:textColor="@color/c_373737"
        android:layout_marginLeft="@dimen/dp_10"
        android:layout_width="wrap_content"
        android:text="@string/agentweb_cancel"
        android:layout_height="wrap_content" />
    <View
        android:layout_below="@+id/text_neirong"
        android:background="@color/c_999999"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_5"/>
</RelativeLayout>