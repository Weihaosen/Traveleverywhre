<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="compp.cumulus.traveleverywhre.activity.UpnameActivity">
<android.support.v7.widget.Toolbar
    android:id="@+id/to"
    android:background="@color/c_FFFFFF"
    android:layout_width="match_parent"
    android:layout_height="@dimen/dp_64">
    <ImageView
        android:id="@+id/back"
        android:layout_marginLeft="@dimen/dp_20"
        android:src="@mipmap/back_white"
        android:layout_width="@dimen/dp_17"
        android:layout_height="@dimen/dp_17" />
    <TextView
        android:layout_marginLeft="@dimen/dp_117"
        android:textSize="@dimen/sp_16"
        android:text="修改名呢"
        android:textColor="@color/c_333333"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
<TextView
    android:id="@+id/te_wancheng"
    android:textColor="@color/c_FA6A13"
    android:text="完成"
    android:layout_marginLeft="@dimen/dp_107"
    android:textSize="@dimen/sp_14"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content" />
</android.support.v7.widget.Toolbar>
    <RelativeLayout
        android:layout_marginLeft="@dimen/dp_10"
        android:background="@color/c_F0F0F0"
        android:layout_marginEnd="@dimen/dp_10"
        android:layout_marginTop="@dimen/dp_84"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_137">

        <EditText
            android:id="@+id/ed_neirong"
            android:gravity="top|left"
            android:background="@color/c_null"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_137"
            android:hint="很关键"/>
        <TextView
            android:id="@+id/shuzi"
            android:layout_marginTop="@dimen/dp_93"
            android:layout_marginEnd="@dimen/dp_22"
            android:layout_alignParentEnd="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
    </RelativeLayout>
</RelativeLayout>
